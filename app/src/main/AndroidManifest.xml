<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.googlesignin">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="CourseTable"
        android:roundIcon="@mipmap/timetable_114px"
        android:icon="@mipmap/timetable_114px"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Weather"></activity>
        <activity android:name=".CourseActivity"></activity>
        <activity android:name=".AddCourseActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>